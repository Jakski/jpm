{:ar "lib.exe"
 :auto-shebang true
 :c++ "cl.exe"
 :c++-link "link.exe"
 :cc "cl.exe"
 :cc-link "link.exe"
 :cflags @["/nologo" "/MD"]
 :cppflags @["/nologo" "/MD" "/EHsc"]
 :cflags-verbose @["-Wall" "-Wextra"]
 :curlpath "curl"
 :dynamic-cflags @["/LD"]
 :dynamic-lflags @["/DLL"]
 :gitpath "git"
 :is-msvc true
 :janet "janet"
 :janet-cflags @[]
 :janet-lflags @[]
 :ldflags @[]
 :lflags @["/nologo"]
 :modext ".dll"
 :nocolor false
 :optimize 2
 :pkglist "https://github.com/janet-lang/pkgs.git"
 :silent false
 :statext ".lib"
 :tarpath "tar"
 :use-batch-shell true
 :verbose false}
